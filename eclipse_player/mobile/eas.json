{
    "cli": {
        "version": ">= 16.25.0",
        "appVersionSource": "app.json"
    },
    "build": {
        "production": {
            "env": {
                "NODE_ENV": "production"
            },
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "buildConfiguration": "Release"
            }
        },
        "development": {
            "env": {
                "NODE_ENV": "development"
            },
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "buildConfiguration": "Debug"
            }
        }
    },
    "submit": {
        "production": {
            "android": {},
            "ios": {}
        }
    }
}